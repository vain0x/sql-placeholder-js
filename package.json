{
  "name": "sql-placeholder-js",
  "version": "1.0.0",
  "description": "Convert SQL placeholders from nominal form (:n) to positional form ($i or ?).",
  "main": "src",
  "scripts": {
    "build": "tsc -b",
    "build-watch": "tsc --preserveWatchOutput --watch",
    "test": "mocha 'src/*.test.js' --reporter dot",
    "test-watch": "mocha 'src/*.test.js' --reporter dot --watch"
  },
  "keywords": [
    "named",
    "nominal",
    "placeholder",
    "parameter",
    "sql"
  ],
  "author": "vain0x <vainzerox@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vain0x/sql-placeholder-js"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^14.11.2",
    "mocha": "^8.1.3",
    "typescript": "^4.0.3"
  }
}
